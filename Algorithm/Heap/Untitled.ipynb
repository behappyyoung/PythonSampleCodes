    "heap=[]\n",
    "import heapq\n",
    "from collections import Counter\n",
    "h = heapq.heapify(heap)\n",
    "\n",
    "l = ['i', 'love', 'i', 'coding', 'love', 'love']\n",
    "\n",
    "wordsDict = Counter(l)\n",
    "\n",
    "print(h, heap, wordsDict)\n",
    "\n",
    "for ll in wordsDict:\n",
    "    heapq.heappush(heap, ll)\n",
    "\n",
    "print(h, heap)"
   ]
